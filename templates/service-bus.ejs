<!-- service bus <%=data.name%> - uses the serviceBus_gradient defined in the header.ejs -->
    <rect x="<%=config.io.serviceBus.startX%>" y="<%=data.y%>" width="<%=config.io.serviceBus.width%>" height="<%=config.io.serviceBus.height%>"
        fill="url(#serviceBus_gradient)" style="fill-opacity:0.6;stroke:rgb(93,119,130);stroke-width:20;opacity:1.0" />
    <text x="<%=config.io.serviceBus.startX + 20%>" y="<%=data.y + config.io.serviceBus.height - 70%>" fill="#005fbf" font-family="Arial, sans-serif" font-size="150px" ><%=data.name%></text>

