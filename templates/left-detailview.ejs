    <!-- DPE -->
    <rect x="<%=config.svg.leftBorder%>" y="<%=config.dpe.y%>" width="<%=config.left.width%>" height="<%=config.dpe.height%>" 
        style="fill:rgb(182,144,73);fill-opacity:1.0;stroke:rgb(134,106,54);stroke-width:20;opacity:1.0" />
    <text x="<%=config.svg.leftBorder + 50%>" y="<%=config.dpe.y + config.dpe.height - 200%>" 
        transform="rotate(-90 <%=config.svg.leftBorder + 150%>,<%=config.dpe.y + config.dpe.height - 200%>)" fill="white" font-family="Arial, sans-serif" font-size="150px">Deployment</text>
    <text x="<%=config.svg.leftBorder + 200%>" y="<%=config.dpe.y + config.dpe.height - 200%>" 
        transform="rotate(-90 <%=config.svg.leftBorder + 300%>,<%=config.dpe.y + config.dpe.height - 200%>)" fill="white" font-family="Arial, sans-serif" font-size="150px">Environments</text>

    <!-- RTE -->
    <rect x="<%=config.svg.leftBorder%>" y="<%=data.rte.y%>" width="<%=config.left.width%>" 
        height="<%=data.rte.height%>" 
        style="fill:rgb(75,145,205);fill-opacity:1.0;stroke:rgb(55,107,151);stroke-width:20;opacity:1.0" />
    <text x="<%=config.svg.leftBorder + 50%>" y="<%=data.rte.y + 20%>" 
        transform="rotate(-90 <%=config.svg.leftBorder + 150%>,<%=data.rte.y + 20%>)" fill="white" font-family="Arial, sans-serif" 
        font-size="150px" text-anchor="end">Runtime</text>
    <text x="<%=config.svg.leftBorder + 200%>" y="<%=data.rte.y + 20%>" 
        transform="rotate(-90 <%=config.svg.leftBorder + 300%>,<%=data.rte.y + 20%>)" fill="white" font-family="Arial, sans-serif" 
        font-size="150px" text-anchor="end">Environments</text>

    
    <text x="<%=config.svg.leftBorder + 20%>" y="<%=data.detail.y + config.left.detailView.height - 50%>" 
        fill="#005fbf" font-family="Arial, sans-serif" font-size="150px" ><%=data.name%></text>
    


    